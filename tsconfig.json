{
  "buildOnSave": false,
  "compileOnSave": false,
  "compilerOptions": {
      "charset": "utf8",
      "module": "commonjs",
      "target": "es2018",
      "lib": [
          "es2018"
      ],
      "noImplicitAny": true,
      "removeComments": true,
      "strictNullChecks": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "preserveConstEnums": true,
      "sourceMap": false,
      "esModuleInterop": false,
      "outDir": "functions",
      "noImplicitReturns": true,
  },
  "typeRoots": [
      "node_modules/@types"
  ]
}